{
  "extends": "airbnb-base",
  "plugins": [ "import" ],
  "rules": {
    "no-underscore-dangle": 0,
    "no-throw-literal":0,
    "prefer-promise-reject-errors": 0,
    "no-param-reassign": 0,
    "object-curly-newline": 1,
    "max-len": 0,
    "consistent-return": 0,
    "no-loop-func": 0,
    "no-await-in-loop": 0,
    "max-lines": 0,
    "no-console": 0,
    "import/newline-after-import": 0,
    "no-unused-vars": 1,
    "camelcase": 0,
    "arrow-body-style": 1,
    "no-return-await": 0,
    "array-callback-return": 0,
    "global-require": 0,
    "linebreak-style": 0,
    "no-unused-expressions": 0,
    "prefer-destructuring": 0,
    "no-plusplus": 0,
    "class-methods-use-this": 0
  },
  "globals": {
    "process": true,
    "__dirname": true
  }
}