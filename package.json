{
  "name": "roo-language-chatbot",
  "version": "1.0.0",
  "description": "Language Chatbot",
  "main": "server.js",
  "scripts": {
    "start": "nodemon --exec babel-node -- src/server.js --copy-files",
    "preprod": "npm run build",
    "prod": "node dist/server.js",
    "lint": "eslint src/",
    "build": "babel src -d dist --copy-files"
  },
  "author": "Andres Hevia",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/langroo/facebook-amazing-bot/issues"
  },
  "engines": {
    "node": "9.3.0",
    "npm": "5.6.0"
  },
  "dependencies": {
    "aws-sdk": "^2.378.0",
    "axios": "^0.16.2",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.11.5",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "busboy": "^0.2.14",
    "connect-busboy": "0.0.2",
    "consolidate": "^0.15.1",
    "cookie-parser": "^1.4.3",
    "cron-builder": "^0.3.0",
    "cron-parser": "^2.7.3",
    "crypto-js": "^3.1.9-1",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "eslint": "^3.2.0",
    "eslint-config-zavatta": "^4.2.0",
    "eslint-plugin-import": "^1.6.1",
    "express": "^4.16.4",
    "express-busboy": "^6.0.1",
    "express-fileupload": "^0.2.0",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "form-data": "^2.3.3",
    "google-translate-api": "^2.3.0",
    "hbs": "^4.0.1",
    "jsonwebtoken": "^8.4.0",
    "messenger-bot": "^2.5.0",
    "mkdirp": "^0.5.1",
    "mongoose": "^5.4.0",
    "morgan": "^1.9.1",
    "node-schedule": "^1.3.1",
    "nodemon": "^1.18.9",
    "raven": "^2.6.4",
    "raven-js": "^3.27.0",
    "recastai": "^4.0.0",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "serve-favicon": "~2.4.2",
    "superagent": "^3.8.3"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "syntax-async-functions"
    ]
  },
  "eslintConfig": {
    "extends": "zavatta",
    "rules": {
      "no-console": 0,
      "callback-return": 0,
      "camelcase": 0,
      "require-await": 0
    }
  }
}
